      "source": [
        "def calculate_total(price, tax_rate=0.10):\n",
        "\n",
        "    total = price + (price * tax_rate)\n",
        "\n",
        "    return total\n",
        "\n",
        "price = 1000\n",
        "\n",
        "total_price = calculate_total(price, tax_rate)\n",
        "\n",
        "print(f\"商品の合計金額は{total_price}円\")"
      ]
